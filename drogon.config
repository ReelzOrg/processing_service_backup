{
  "app": {
    "threads": 4,
    "logLevel": "INFO",
    "enableSession": false,
    "sessionTimeout": 3600,
    "documentRoot": "./public",
    "uploadPath": "./uploads"
  },
  "listeners": [
    {
      "address": "0.0.0.0",
      "port": 5555
    }
  ],
  "plugins": {
    "KafkaConsumerPlugin": {
      "topic": "media_processing",
      "brokers": "localhost:9092",
      "group_id": "media-consumer-group",
      "poll_ms": 100,
      "enable_auto_commit": false,
      "auto_offset_reset": "latest"
    }
  }
}